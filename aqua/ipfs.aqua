data IpfsResult:
  success: bool
  result: string

service Ipfs("ipfs-adapter"):
  get_external_multiaddr() -> IpfsResult
  get_from(hash: string, multiaddr: string) -> IpfsResult
  get_local_multiaddr() -> IpfsResult
  invoke() -> string
  put(file_path: string) -> IpfsResult
  set_external_multiaddr(multiaddr: string) -> IpfsResult
  set_local_multiaddr(multiaddr: string) -> IpfsResult
  set_timeout(timeout_sec: u64)  
